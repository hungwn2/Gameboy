#cinldue "spi.h"
#define SPI1EN (1U<<12)
#define GPIOAEN (1U<<0)
#define SR_TXE (1U<<1)
#define SR_RXNE (1U<<0)
#define SR_BSY	(1U<<7)


void spi_gpio_init(void){
	RCC->AHB1ENR| =GPIOAEN;

	GPIOA->MODER &=~(1U<<10);
	GPIOA->MODER |=(1U<<11);
	GPIOA->MODER &=~(1U<<12);
	GPIOA->MODER |=(1U<<13);
	GPIOA->MODER &=~(1U<<14);
	GPIOA->MODER |=(1U<<15);
	GPIOA->MODER &=~(1U<<18);
	GPIOA->MODER |=(1U<<19);

}
