#include "../inc/my_vector.h"



my_vector NewVector(int32_t x, int32_t y){
	my_vector v={.x=x, .y=y};
	return v;
}

my_vector Vectorf2Vector(my_vectorf v){
    return NewVector((int32_t)v.x, (int32_t)v.y);
}


my_vectorf NewVectorf(float x, float y){
	my_vectorf v={.x=x, .y=y};
	return v;
}

my_vector VectorAdd(my_vector v, my_vector w){
    return NewVector(v.x + w.x, v.y + w.y);
}

my_vectorf VectorfAdd(my_vectorf v, my_vectorf w){
    return NewVectorf(v.x + w.x, v.y + w.y);
}

my_vectorf VectorffAdd(my_vectorf v, my_vectorf w){
    return NewVectorf(v.x + w.x, v.y + w.y);
}

my_vectorf VectorfMul(my_vectorf v, my_vectorf t){
	return NewVectorf(v.x * t, v.y * t);
}
